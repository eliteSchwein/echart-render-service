{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "galex"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
    ],
    "rules": {
        "require-unicode-regexp": "off",
        "no-console": "off",
        "unicorn/no-process-exit": "off"
    }
}
